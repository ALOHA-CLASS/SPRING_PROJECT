<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aloha.shop.???.mapper.???Mapper">

    <!-- 목록 -->
    <select id="list" resultType="">
        SELECT *
        FROM ???
    </select>

    <!-- 조회 -->
    <select id="select" resultType="">
        SELECT *
        FROM ???
        WHERE id = #{id}
    </select>

    <!-- 등록 -->
    <insert id="insert">
        INSERT INTO ??? ( ???, ??? )
        VALUES ( #{???}, #{???} )
    </insert>

    <!-- 수정 -->
    <update id="update">
        UPDATE ???
           SET ??? = #{???}
              ,updated_at = now()
        WHERE id = #{id}
    </update>

    <!-- 삭제 -->
    <delete id="delete">
        DELETE FROM ???
        WHERE id = #{id}
    </delete>

</mapper>